<UserControl x:Class="Quick_Solutions_Start_Page_v2_0Control.ConfigureButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vs="clr-namespace:Microsoft.VisualStudio.PlatformUI;assembly=Microsoft.VisualStudio.Shell.10.0"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d"
             d:DesignHeight="300"
             d:DesignWidth="300">
    <vs:ImageButton x:Uid="OpenConfigureButton"
                    Margin="15,2,0,2"
                    Width="Auto"
                    Content="Configuration..."
                    Click="ImageButton_Click"
                    Style="{DynamicResource StartPage.ProjectCommand.ButtonStyle}"
                    ImageNormal="pack://application:,,,/QuickSolutionsStartPagev20Control;component/configuration-icon.png"
                    ImageHover="pack://application:,,,/QuickSolutionsStartPagev20Control;component/configuration-icon.png"
                    ImagePressed="pack://application:,,,/QuickSolutionsStartPagev20Control;component/configuration-icon.png">
    </vs:ImageButton>
</UserControl>
